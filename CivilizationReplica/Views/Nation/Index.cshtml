@model CivilizationReplica.Models.Nation

<head>
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
     <script type="text/javascript">
		
        $(document).ready(function () {
            $('.play').click(function () {
                 $.ajax({
                    type: 'GET',
                    dataType: 'html',
                    url: '@Url.Action("Game")',
                    success: function (result) {
                        $('#nationInfo').html(result);
                     }
                    });
              });
		});
	</script>
</head>
<body>
	<button class="btn btn-lg play" type="button">Play</button>
	<div id="nationInfo"></div>
</body>


